<?xml version="1.0" encoding="utf-8"?>
<Mod id="mws-settlement-limit-settings" version="1"
	xmlns="ModInfo">
	<Properties>
		<Name>LOC_MODULE_SETTLEMENT_LIMIT_SETTINGS</Name>
		<Description>LOC_MODULE_SETTLEMENT_LIMIT_SETTINGS_DESC</Description>
		<Authors>Michael Wagner</Authors>
		<ShowInBrowser>1</ShowInBrowser>
		<Package>MW's Settlement Limit Settings</Package>
    <AffectsSavedGames>1</AffectsSavedGames>
    <EnabledByDefault>1</EnabledByDefault>
    <Version>1.0.1</Version>
		<URL>https://github.com/MSWagner/civ7-mod-settlement-limit</URL>
	</Properties>
	<Dependencies>
		<Mod id="base-standard" title="LOC_MODULE_BASE_STANDARD_NAME"/>
    <Mod id="age-antiquity" title="LOC_MODULE_AGE_ANTIQUITY_NAME"/>
		<Mod id="age-exploration" title="LOC_MODULE_AGE_EXPLORATION_NAME"/>
		<Mod id="age-modern" title="LOC_MODULE_AGE_MODERN_NAME"/>
	</Dependencies>
	<ActionCriteria>
		<Criteria id="always">
			<AlwaysMet/>
		</Criteria>

    <!-- Double Settlement Limit -->
    <Criteria id="double-settlement-limit-antiquity">
      <AgeInUse>AGE_ANTIQUITY</AgeInUse>
			<ConfigurationValueMatches>
				<Group>Game</Group>
				<ConfigurationId>MWS-SettlementLimitType</ConfigurationId>
				<Value>MWS_SETTLEMENT_LIMIT_DOUBLE</Value>
			</ConfigurationValueMatches>
		</Criteria>
